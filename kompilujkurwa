menhir --dump --explain --infer parser.mly
ocamlc -c parser.mli
ocamlc -c parser.ml
ocamllex lexer.mll
ocamlc -c lexer.ml
ocamlc -c syntax.mli
ocamlc -c syntax.ml
ocamlc -c printer.mli
ocamlc -c printer.ml
ocamlc -c machine.mli
ocamlc -c machine.ml
ocamlc -c main.ml
ocamlc -o main parser.cmo lexer.cmo syntax.cmo printer.cmo machine.cmo main.cmo
